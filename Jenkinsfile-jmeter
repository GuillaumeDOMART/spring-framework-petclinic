pipeline {
    agent any
    tools {
        // Install the Maven  version configured as "Maven3.9.12" and add it to the path.
        maven "Maven3.9.12"
        jdk 'Corretto-17'
    }
    stages {
        stage('Build') {
            steps {
                sh "jmeter -Jjmeter.saveservice.ouput_format=xml -Jjmeter.saveservice.responce_data.on_error=true -n -t HTTP_Request.jmx -l testresult.jlt"
            }
        }
    }
}
